package com.easycart.controller.admin.category;

import java.util.List;

import javax.validation.Valid;

/**
 * 分类表单
 * @author yaoliang
 *
 */
public class CategoryView {

	@Valid
	private List<MenuForm> menuList;

	@Valid
	private List<BrandForm> brandFormList;
	
	public List<MenuForm> getMenuList() {
		return menuList;
	}

	public void setMenuList(List<MenuForm> menuList) {
		this.menuList = menuList;
	}

	public List<BrandForm> getBrandFormList() {
		return brandFormList;
	}

	public void setBrandFormList(List<BrandForm> brandFormList) {
		this.brandFormList = brandFormList;
	}
	
	

	
}
