package com.easycart.utils;

/**
 * 状态数值
 * @author yaoliang
 *
 */
public enum Status {

	status_0((byte)0), status_1((byte)1), status_2((byte)2),status_3((byte)3);
	
	private byte value;
	
	private Status(byte b){
		this.value = b;
	}

	public byte getValue() {
		return value;
	}

	public void setValue(byte value) {
		this.value = value;
	}

	
	
}
