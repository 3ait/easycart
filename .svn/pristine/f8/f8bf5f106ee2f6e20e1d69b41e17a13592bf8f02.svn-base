	audio,
	video {
		margin: 0;
		padding: 0;
		border: 0;
		font: inherit;
		font-size: 100%;
		vertical-align: baseline;
	}
	
	body {
		cursor: default;
		font-family: "Helvetica Neue", Helvetica, Roboto, Arial, "PingFang SC", "Noto Sans CJK SC", "Source Han Sans CN", "Lantinghei SC", "Microsoft Yahei", "Hiragino Sans GB", "WenQuanYi Micro Hei", sans-serif;
		/*letter-spacing: 1px;*/
	}
	
	img {
		cursor: pointer;
		border: none;
		font-size: 0;
		display: block;
	}
	
	li {
		list-style: none;
	}
	
	a {
		text-decoration: none;
		color: black;
	}
	
