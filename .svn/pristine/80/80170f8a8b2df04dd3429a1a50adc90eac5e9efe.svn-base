html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
	margin: 0;
	padding: 0;
	border: 0;
	font: inherit;
	font-size: 100%;
	vertical-align: baseline;
}

body {
	font-family: "AdobeFanHeitiStd" "Helvetica Neue", Helvetica, Roboto, Arial, "PingFang SC", "Noto Sans CJK SC", "Source Han Sans CN", "Lantinghei SC", "Microsoft Yahei", "Hiragino Sans GB", "WenQuanYi Micro Hei", sans-serif;
	cursor: default;
}

li {
	list-style: none;
}

input {
	font-size: 18px;
}

#apContainer {
	width: 100%;
	display: flex;
	font-size: 18px;
}

.aside {
	width: 150px;
	height: 1050px;
	background: #E7E7E7;
	padding-top: 40px;
}

.flex1 {
	flex: 1;
}

.aNav {
	width: 150px;
	height: 50px;
	line-height: 50px;
	text-align: center;
	/*background: red;*/
}

section {
	font-size: 18px;
	padding: 45px 0 36px 0;
	flex: 10;
	/*background: red;*/
}


/*admin_product*/

.search {
	height: 65px;
	/*background: red;*/
	display: flex;
	align-items: center;
}

.search input {
	width: 300px;
	height: 24px;
}

.search div {
	width: 33px;
	height: 30px;
	background: #E7E7E7;
	line-height: 40px;
	text-align: center;
	margin: 20px;
}


/*table*/

.apAll {
	font-size: 10px;
	/*background: gray;*/
	height: 500px;
}

.tHeader {
	display: flex;
	height: 30px;
	border-bottom: 1px solid #FFFFFF;
}

.tHeader div {
	background: #f7e07f;
	/*text-align: center;
	line-height: 30px;*/
	display: flex;
	justify-content: center;
	align-items: center;
	border-right: 1px solid #FFFFFF;
}

.tHeaderL {
	width: 65px;
}

form {
	border-bottom: 1px solid #FFFFFF;
}

.tContent {
	/*background: red;*/
	display: flex;
	align-items: center;
}

.tContent>div {
	display: flex;
	justify-content: center;
	align-items: center;
	height: 30px;
	background: #d7d7d7;
	border-right: 1px solid #FFFFFF;
}

.tContent input {
	border: none;
}

.tHeaderL input {
	padding: 0;
	background: #d7d7d7;
	width: 65px;
	height: 30px;
	font-size: 10px;
}

.tHeaderLL {
	width: 150px;
}

.tHeaderLL input {
	background: #d7d7d7;
	width: 148px;
	height: 28px;
	font-size: 10px;
}

.tBtnA {
	border: none;
	width: 40px;
	height: 28px;
	font-size: 12px;
	line-height: 20px !important;
	margin-left: 20px;
}

.tBtn {
	border: none;
	width: 40px;
	height: 28px;
	font-size: 12px;
	line-height: 20px !important;
	margin-left: 20px;
}

.apPage {
	/*background: red;*/
	display: flex;
	align-items: center;
	justify-content: flex-end;
}

.pageBtn {
	padding: 3px 5px;
	text-align: center;
	margin: 0 10px;
	width: 75px;
	height: 20px;
	background: #E7E7E7;
}

#pageGo {
	width: 26px;
	height: 16px;
}